<Window
    x:Class="AinAlAtaaFoundation.Features.FamiliesManagement.Print.View"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:boldReports="clr-namespace:BoldReports.UI.Xaml;assembly=BoldReports.Wpf"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AinAlAtaaFoundation.Features.FamiliesManagement.Print"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="View"
    WindowStartupLocation="CenterScreen"
    WindowState="Maximized"
    mc:Ignorable="d">
    <Grid>
        <boldReports:ReportViewer
            Name="_reportViewer"
            Grid.Row="2"
            ProcessingMode="Local"
            ShowParametersBlock="False"
            ShowRefreshButton="False"
            ViewMode="Print" />
    </Grid>
</Window>
