<Window
    x:Class="AinAlAtaaFoundation.Features.FamiliesManagement.Members.ListingWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AinAlAtaaFoundation.Features.FamiliesManagement.Members"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="أعضاء العائلة"
    d:DataContext="{d:DesignInstance Type=local:ListingWindow}"
    x:ClassModifier="internal"
    FontFamily="Arial"
    FontSize="16"
    FontWeight="Bold"
    SizeToContent="WidthAndHeight"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Border>
        <ContentControl Content="{Binding View}" />
    </Border>
</Window>
