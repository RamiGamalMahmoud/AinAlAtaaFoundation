<Window
    x:Class="AinAlAtaaFoundation.Services.Printing.BoldReportsViewerWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:boldReports="clr-namespace:BoldReports.UI.Xaml;assembly=BoldReports.Wpf"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AinAlAtaaFoundation.Services.Printing"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="طباعة"
    WindowStartupLocation="CenterScreen"
    WindowState="Maximized"
    mc:Ignorable="d">

    <boldReports:ReportViewer
        Name="reportViewer"
        Grid.Row="2"
        ProcessingMode="Local"
        ShowParametersBlock="False"
        ShowRefreshButton="False"
        ViewMode="Print" />

</Window>
